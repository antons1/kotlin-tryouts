type Query {
    books: [Book]
    bookById(id: ID!): Book
    authors: [Author]
    authorById(id: ID!): Author
}

type Book {
    id: ID!
    name: String!
    pageCount: Int!
    author: Author!
}

type Author {
    id: ID!
    firstName: String!
    lastName: String!
    books: [Book]
}